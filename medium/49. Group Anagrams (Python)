class Solution(object):
    def groupAnagrams(self, strs):
        """
        :type strs: List[str]
        :rtype: List[List[str]]
        """
        
        ans = defaultdict(list)

        for i in strs:
            k = "".join(sorted(i))
            ans[k].append(i)
            
        return list(ans.values())
