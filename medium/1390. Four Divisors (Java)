class Solution {
    public int getSumDivisorsOfFour(int num) {
        int sum = 0;
        int divisors = 0;

        for (int i = 1; i <= num; i++) {
            if (num % i == 0) {
                sum += i;
                divisors++;
            }

            if (divisors > 4) return 0;
        }

        if (divisors != 4) return 0;
        return sum;
    }
    public int sumFourDivisors(int[] nums) {
        int sum = 0;

        for (int i = 0; i < nums.length; i++) {
            sum += getSumDivisorsOfFour(nums[i]);
        }

        return sum;
    }
}
