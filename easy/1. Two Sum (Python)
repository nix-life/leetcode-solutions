class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """

        map =  {}

        for i, v in enumerate(nums):
            if target - v in map:
                return i, map[target-v]

            else:
                map[v] = i
